<Window x:Class="ObjectsCreator.Authorization"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ObjectsCreator"
        mc:Ignorable="d"
        Title="Authorization" Height="168" Width="234">
    <Grid >
 
        <PasswordBox x:Name="Password" Margin="20,64,49,78" Grid.Column="1" />
        <TextBox x:Name="Login" Margin="20,49,49,93" RenderTransformOrigin="0.5,0.5" Grid.Column="1">

        </TextBox>
        <Button Visibility="Hidden" x:Name="Register" Content="Register" Margin="60,109,84,23" Click="Register_Click" Grid.Column="1"/>
        <Button x:Name="LoginButton" Content="Login" Margin="60,84,84,48" Click="Login_Click" Grid.Column="1"/>

    </Grid>
</Window>
