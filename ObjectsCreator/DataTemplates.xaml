<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    
             xmlns:local="clr-namespace:ObjectsCreator.MVVM.Views"
             xmlns:views="clr-namespace:ObjectsCreator.MVVM.Models">
    <DataTemplate DataType="{x:Type views:AuthorizationViewModel}">
        <local:AuthorizationView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type views:MainContentViewModel}">
        <local:MainContentView/>
    </DataTemplate>
</ResourceDictionary>